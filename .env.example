# Environment variables for cybersecurity web automation tool

# Proxy Configuration
PROXY_ENABLED=false
PROXY_LIST=http://proxy1:8080,http://proxy2:8080
PROXY_ROTATION_INTERVAL=5

# Browser Configuration
HEADLESS_MODE=false
USER_AGENT_ROTATION=true
STEALTH_MODE=true

# Timing Configuration
MIN_SCROLL_DELAY=1.0
MAX_SCROLL_DELAY=3.0
MIN_CLICK_DELAY=0.5
MAX_CLICK_DELAY=2.0
PAGE_LOAD_TIMEOUT=30

# Security Settings
VERIFY_SSL=true
REQUEST_TIMEOUT=10

# Target Configuration (for testing purposes only)
TARGET_URLS=https://example.com,https://httpbin.org

# Logging
LOG_LEVEL=INFO
LOG_FILE=automation.log
